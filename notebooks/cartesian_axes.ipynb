   "source": [
    "# Create the cartesian axis\n",
    "axes = Axes(xlim=(-1,8), ylim=(-1,18), figsize=(9,7))\n",
    "\n",
    "# Create two points\n",
    "p1 = Point(2,  5, color='#ffa500')\n",
    "p2 = Point(7, 17, color='#0000ff')\n",
    "\n",
    "axes.addMany([p1, p2])\n",
    "axes.draw()"
